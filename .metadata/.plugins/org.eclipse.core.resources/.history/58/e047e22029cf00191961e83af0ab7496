package projet1;

public class Ville {
	String nomVille;
	String nomPays;
	int nbreHabitants;
	
	public Ville() 
	{
		System.out.println("Création d'une ville!");
		nomVille = "Inconnu";
		nomPays = "Inconnu";
		nbreHabitants = 0;
	}
	
	public Ville(String pNom, int pNbre, String pPays) 
	{
		System.out.println("Création d'une ville avec des paramètres !");
		nomVille = pNom;
		nomPays = pPays;
		nbreHabitants = pNbre;
	}
}
